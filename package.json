{
  "name": "turnip-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build:watch": "npm run build -- --watch",
    "build": "rimraf dist/ && tsc --project tsconfig.json",
    "lint": "eslint '**/*.{js,ts,tsx}' --quiet --fix",
    "start": "node dist/index.js",
    "start:dev": "nodemon --inspect --watch dist dist/index.js",
    "env:local": "docker-compose -f docker-compose.yml -f dev-compose.yml up --build -d"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/ioredis": "^4.14.9",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.5",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
