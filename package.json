{
  "name": "turnip-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build:watch": "npm run build:prod -- --watch",
    "build": "rimraf dist/ && tsc --project tsconfig.json",
    "build:prod": "rimraf dist/ && tsc --project tsconfig-build.json",
    "lint": "eslint '**/*.ts' --quiet --fix",
    "start": "node dist/index.js",
    "start:dev": "nodemon --inspect --watch dist dist/index.js",
    "env:local": "npm run build:prod && docker-compose -f docker-compose.yml -f dev-compose.yml up --build -d"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/ioredis": "^4.16.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "ioredis-mock": "^4.19.0",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-mockito": "^2.5.0",
    "typescript": "^3.8.3",
    "url-parse": "^1.4.7"
  },
  "dependencies": {
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "ioredis": "^4.16.3",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.24"
  }
}
